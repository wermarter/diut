  "buildCommand": "cd ../../libs/common && yarn build && cd ../../apps/access-server && yarn build",
  "outputDirectory": "dist",
  "functions": {
    "dist/main.js": {
      "includeFiles": "dist/**/*"
    }
  },
  "rewrites": [{ "source": "/(.*)", "destination": "/main.js" }]
}
