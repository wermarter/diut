{
  "name": "@diut/puppeteer-service",
  "description": "Puppeteer service",
  "repository": "https://github.com/wermarter/diut",
  "author": "Ha Minh Chien <wermarter@gmail.com>",
  "private": true,
  "main": "dist/index",
  "types": "dist/index",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && nest build",
    "dev": "nest start --watch --preserveWatchOutput",
    "lint": "tsc && rimraf dist",
    "proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto -I=src/proto --ts_proto_out=src/proto/ src/proto/package.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb --experimental_allow_proto3_optional"
  },
  "dependencies": {
    "@diut/common": "workspace:*",
    "@diut/nest-core": "workspace:*",
    "@grpc/grpc-js": "^1.9.12",
    "@grpc/proto-loader": "^0.7.10",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/common": "^10.2.8",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/microservices": "^10.2.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "puppeteer": "^21.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.3",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.165.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  }
}
